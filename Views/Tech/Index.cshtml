@model IEnumerable<MingleBoxProject.Models.tblTech>

@{
    ViewBag.Title = "Index"; Layout = "~/Views/Shared/main.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.techname)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.techname)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.techid }) |
            @Html.ActionLink("Details", "Details", new { id=item.techid }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.techid })
        </td>
    </tr>
}

</table>
